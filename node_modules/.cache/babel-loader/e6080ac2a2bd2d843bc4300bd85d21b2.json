{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'IniciarSesion',\n\n  data() {\n    return {\n      usuario: '',\n      contraseña: '',\n      errors: []\n    };\n  },\n\n  mounted() {\n    document.title = \"Iniciar Sesión | Cesfam\";\n  },\n\n  methods: {\n    submitForm() {\n      axios.defaults.headers.common['Authorization'] = \"\"; ///reseteamos y borramos el token para que no ocurra ningun error\n\n      localStorage.removeItem('token');\n      const formData = {\n        username: this.usuario,\n        password: this.contraseña\n      };\n      axios.post('/api/v1/token/login/', formData).then(response => {\n        const token = response.data.auth_token;\n        this.$store.commit('setToken', token);\n        axios.defaults.headers.common[\"Authorization\"] = \"Token \" + token;\n        localStorage.setItem(\"token\", token);\n        const toPath = this.$route.query.to || '/carrito';\n        this.$route.push(toPath);\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACXC,IAAI,EAAE,eADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EADN;MAEHC,UAAU,EAAE,EAFT;MAGHC,MAAM,EAAE;IAHL,CAAP;EAKH,CARU;;EASXC,OAAO,GAAG;IACNC,QAAQ,CAACC,KAAT,GAAiB,yBAAjB;EACH,CAXU;;EAYXC,OAAO,EAAE;IACLC,UAAU,GAAE;MACRV,KAAK,CAACW,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiD,EAAjD,CADQ,CAC4C;;MAEpDC,YAAY,CAACC,UAAb,CAAwB,OAAxB;MAEA,MAAMC,QAAO,GAAI;QACbC,QAAQ,EAAE,KAAKd,OADF;QAEbe,QAAQ,EAAE,KAAKd;MAFF,CAAjB;MAKAJ,KAAI,CACCmB,IADL,CACU,sBADV,EACkCH,QADlC,EAEKI,IAFL,CAEUC,QAAO,IAAK;QACd,MAAMC,KAAI,GAAID,QAAQ,CAACnB,IAAT,CAAcqB,UAA5B;QAEA,KAAKC,MAAL,CAAYC,MAAZ,CAAmB,UAAnB,EAA+BH,KAA/B;QAEAtB,KAAK,CAACW,QAAN,CAAeC,OAAf,CAAuBC,MAAvB,CAA8B,eAA9B,IAAiD,WAAWS,KAA5D;QAEAR,YAAY,CAACY,OAAb,CAAqB,OAArB,EAA8BJ,KAA9B;QAEA,MAAMK,MAAK,GAAI,KAAKC,MAAL,CAAYC,KAAZ,CAAkBC,EAAlB,IAAwB,UAAvC;QAEA,KAAKF,MAAL,CAAYG,IAAZ,CAAiBJ,MAAjB;MACH,CAdL;IAeJ;;EA1BK;AAZE,CAAf","names":["axios","name","data","usuario","contraseña","errors","mounted","document","title","methods","submitForm","defaults","headers","common","localStorage","removeItem","formData","username","password","post","then","response","token","auth_token","$store","commit","setItem","toPath","$route","query","to","push"],"sourceRoot":"","sources":["/Users/franciscosantander/Desktop/Cesfam/cesfam_vue/src/views/IniciarSesion.vue"],"sourcesContent":["<template>\n    <div class=\"page-iniciar-sesion\">\n        <h1 class=\"title\">Iniciar Sesión</h1>\n\n        <form @submit.prevent=\"submitForm\">\n            <div class=\"field\">\n                <label>Nombre de usuario</label>\n                <div class=\"control\">\n                    <input type=\"text\" class=\"input\" v-model=\"usuario\">\n                </div>\n            </div>\n\n            <div class=\"field\">\n                <label>Contraseña</label>\n                <div class=\"control\">\n                    <input type=\"password\" class=\"input\" v-model=\"contraseña\">\n                </div>\n            </div>\n\n            <div class=\"notification is-danger\" v-if=\"errors.length\">\n                <p v-for=\"error in errors\" v-bind:key=\"error\">{{ error }}</p>\n            </div>\n\n            <div class=\"field\">\n                <div class=\"control\">\n                    <button class=\"btn btn-outline-primary\">Iniciar Sesión</button>\n                </div>\n            </div>\n\n            <hr>\n\n            O <router-link to=\"/registro\">Precione aquí</router-link> para Registarse\n        </form>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nexport default {\n    name: 'IniciarSesion',\n    data() {\n        return {\n            usuario: '',\n            contraseña: '',\n            errors: []\n        }\n    },\n    mounted() {\n        document.title = \"Iniciar Sesión | Cesfam\"\n    },\n    methods: {\n        submitForm(){\n            axios.defaults.headers.common['Authorization'] = \"\" ///reseteamos y borramos el token para que no ocurra ningun error\n\n            localStorage.removeItem('token')\n\n            const formData = {\n                username: this.usuario,\n                password: this.contraseña\n            }\n\n            axios \n                .post('/api/v1/token/login/', formData)\n                .then(response => {\n                    const token = response.data.auth_token\n\n                    this.$store.commit('setToken', token)\n\n                    axios.defaults.headers.common[\"Authorization\"] = \"Token \" + token\n\n                    localStorage.setItem(\"token\", token)\n\n                    const toPath = this.$route.query.to || '/carrito'\n\n                    this.$route.push(toPath)\n                })\n        }\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}