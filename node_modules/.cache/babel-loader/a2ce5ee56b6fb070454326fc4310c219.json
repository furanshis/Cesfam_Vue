{"ast":null,"code":"import axios from 'axios';\nimport { toast } from 'bulma-toast';\nexport default {\n  name: \"Detalle_remedio\",\n\n  data() {\n    return {\n      remedio: {},\n      quantity: 1\n    };\n  },\n\n  mounted() {\n    this.getRemedio();\n  },\n\n  methods: {\n    getRemedio() {\n      const categoria_slug = this.$route.params.categoria_slug;\n      const remedio_slug = this.$route.params.remedio_slug;\n      axios.get(`/api/v1/remedios/${categoria_slug}/${remedio_slug}`).then(response => {\n        this.remedio = response.data;\n        document.title = this.remedio.nombreRemedio + ' | Cesfam';\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    addToCart() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n\n      const item = {\n        remedio: this.remedio,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', item);\n      toast({\n        message: 'El producto fue a単adido al carrito',\n        type: 'is-success',\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: 'bottom-right'\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuFA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,KAAT,QAAsB,aAAtB;AACA,eAAe;EACXC,IAAI,EAAE,iBADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EADN;MAEHC,QAAQ,EAAE;IAFP,CAAP;EAIH,CAPU;;EAQXC,OAAO,GAAG;IACN,KAAKC,UAAL;EACH,CAVU;;EAWXC,OAAO,EAAE;IACLD,UAAU,GAAG;MACT,MAAME,cAAa,GAAI,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,cAA1C;MACA,MAAMG,YAAW,GAAI,KAAKF,MAAL,CAAYC,MAAZ,CAAmBC,YAAxC;MACAZ,KAAI,CACCa,GADL,CACU,oBAAmBJ,cAAe,IAAGG,YAAa,EAD5D,EAEKE,IAFL,CAEUC,QAAO,IAAK;QACd,KAAKX,OAAL,GAAeW,QAAQ,CAACZ,IAAxB;QACAa,QAAQ,CAACC,KAAT,GAAiB,KAAKb,OAAL,CAAac,aAAb,GAA6B,WAA9C;MACH,CALL,EAMKC,KANL,CAMWC,KAAI,IAAK;QACZC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACH,CARL;IASH,CAbI;;IAcLG,SAAS,GAAG;MACR,IAAIC,KAAK,CAAC,KAAKnB,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAgB,CAA5C,EAA+C;QAC3C,KAAKA,QAAL,GAAgB,CAAhB;MACJ;;MACA,MAAMoB,IAAG,GAAI;QACTrB,OAAO,EAAE,KAAKA,OADL;QAETC,QAAQ,EAAE,KAAKA;MAFN,CAAb;MAIA,KAAKqB,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAAgCF,IAAhC;MACAxB,KAAK,CAAC;QACF2B,OAAO,EAAE,oCADP;QAEFC,IAAI,EAAE,YAFJ;QAGFC,WAAW,EAAE,IAHX;QAIFC,YAAY,EAAE,IAJZ;QAKFC,QAAQ,EAAE,IALR;QAMFC,QAAQ,EAAE;MANR,CAAD,CAAL;IAQJ;;EA/BK;AAXE,CAAf","names":["axios","toast","name","data","remedio","quantity","mounted","getRemedio","methods","categoria_slug","$route","params","remedio_slug","get","then","response","document","title","nombreRemedio","catch","error","console","log","addToCart","isNaN","item","$store","commit","message","type","dismissible","pauseOnHover","duration","position"],"sourceRoot":"","sources":["/Users/franciscosantander/Desktop/Cesfam/cesfam_vue/src/views/Detalle_remedio.vue"],"sourcesContent":["<template>\n    <div class=\"detalle_remedio\">\n        <div class=\"columns is-multiline\">\n            <!--imagen  -->\n            <div class=\"card\" style=\"width: 33rem;\">\n                <img class=\"card-img-top\" v-bind:src=\"remedio.get_thumbnail\" alt=\"\" loading=\"lazy\" width=\"500\"\n                    height=\"500\">\n            </div>\n            <!--tarjeta descripcion  -->\n            <div class=\"card\" style=\"width: 44rem;\">\n                <div class=\"card-body\">\n                    <h5 class=\"card-title\">\n                        <h2 class=\"subtitle\"></h2>\n                    </h5>\n                    <h6 class=\"card-subtitle mb-2 text-muted\">\n                        <h2 class=\"subtitle\">{{ remedio.nombreRemedio }}</h2>\n                    </h6>\n                    <p class=\"card-text\">{{ remedio.descripcionRemedio }}.</p>\n                    <p><strong>Precio: </strong>$ {{ remedio.precioRemedio }}</p>\n                    <p><strong>Cantidad: </strong>{{ remedio.cantidadRemedio }}</p>\n                    <p><strong>Stock: </strong>{{ remedio.stockRemedio }}</p>\n\n                    <!--selector cantidad  -->\n                    <div class=\"col-md-12 overflow-needed\">\n                        <div class=\"col-xs-6 col-sm-6 col-md-3 col-lg-2 pfr\">\n                            <div class=\"quantity\">\n                                <label>\n                                    Cantidad\n                                </label>\n                                <div class=\"input-group\">\n                                    <i class=\"fa fa-chevron-up up\" id=\"quantity-up\">\n                                    </i><input type=\"number\" class=\"input\" min=\"1\" v-model=\"quantity\"\n                                        id=\"product-quantity\">\n                                    <i class=\"fa fa-chevron-down down\" id=\"quantity-down\">\n                                    </i>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n\n                    \n                 <div class=\"row m-0\" style=\"width: 20rem; height: 1rem;\">\n                        <div class=\"col-xs-12 col-md-6 pl-0\">\n                            <div class=\"shipment-methods pickup pickup_delivery\">\n                                <div class=\"shipment-methods-title-desc\" >\n                                    <div class=\"asd\" >Retiro en farmacia </div>\n                                    <div class=\"desc\">Disponible</div>\n                                </div>\n                            </div>\n                        </div>\n                        <div class=\"col-xs-12 col-md-6 px-0\">\n                            <div class=\"shipment-methods home_delivery mt-sm package_delivery\">\n                                <div class=\"shipment-methods-title-desc\">\n                                    <div class=\"asd\">Despacho a domicilio</div>\n                                    <div class=\"desc\">Disponible</div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n\n       \n                    <!--boton a単adir-->\n                    <div class=\"control\">\n                        <br>\n                        <div class=\"add-to-cart\"><button class=\"btn btn-primary\" data-disable-with=\"Agregando ...\"\n                                id=\"add-to-cart-button\" @click=\"addToCart\">A単adir al carro<span><img\n                                        src=\"https://d1tjllbjmslitt.cloudfront.net/assets/shopping_cart_icon_white-e9d947ffee21ece11fe275b55f3ebc35cd7979255ec6d4f4bdda66721efa350e.svg\"\n                                        alt=\"Shopping cart icon white\" width=\"13\" height=\"15\"></span></button>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n\n\n\n      \n\n\n\n</template>\n\n\n\n\n<script>\nimport axios from 'axios'\nimport { toast } from 'bulma-toast'\nexport default {\n    name: \"Detalle_remedio\",\n    data() {\n        return {\n            remedio: {},\n            quantity: 1\n        }\n    },\n    mounted() {\n        this.getRemedio()\n    },\n    methods: {\n        getRemedio() {\n            const categoria_slug = this.$route.params.categoria_slug\n            const remedio_slug = this.$route.params.remedio_slug\n            axios\n                .get(`/api/v1/remedios/${categoria_slug}/${remedio_slug}`)\n                .then(response => {\n                    this.remedio = response.data\n                    document.title = this.remedio.nombreRemedio + ' | Cesfam'\n                })\n                .catch(error => {\n                    console.log(error)\n                })\n        },\n        addToCart() {\n            if (isNaN(this.quantity) || this.quantity < 1) {\n                this.quantity = 1\n            }\n            const item = {\n                remedio: this.remedio,\n                quantity: this.quantity,\n            }\n            this.$store.commit('addToCart', item)\n            toast({\n                message: 'El producto fue a単adido al carrito',\n                type: 'is-success',\n                dismissible: true,\n                pauseOnHover: true,\n                duration: 2000,\n                position: 'bottom-right',\n            })\n        }\n    },\n}\n</script>"]},"metadata":{},"sourceType":"module"}