{"ast":null,"code":"import axios from 'axios';\nimport RemediosBox from '@/components/RemediosBox.vue';\nexport default {\n  name: 'Search',\n  components: {\n    RemediosBox\n  },\n\n  data() {\n    return {\n      remedios: [],\n      query: ''\n    };\n  },\n\n  mounted() {\n    document.title = 'Busqueda | Cesfam';\n    let uri = window.location.search.substring(1);\n    let params = new URLSearchParams(uri);\n\n    if (params.get('query')) {\n      this.query = params.get('query');\n      this.performSearch();\n    }\n  },\n\n  methods: {\n    performSearch() {\n      axios.post('/api/v1/remedios/search/', {\n        'query': this.query\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmBA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,eAAe;EACXC,IAAI,EAAE,QADK;EAEXC,UAAU,EAAE;IACRF;EADQ,CAFD;;EAKXG,IAAI,GAAE;IACF,OAAO;MACHC,QAAQ,EAAE,EADP;MAEHC,KAAK,EAAE;IAFJ,CAAP;EAIH,CAVU;;EAWXC,OAAO,GAAG;IACNC,QAAQ,CAACC,KAAT,GAAiB,mBAAjB;IAEA,IAAIC,GAAE,GAAIC,MAAM,CAACC,QAAP,CAAgBC,MAAhB,CAAuBC,SAAvB,CAAiC,CAAjC,CAAV;IACA,IAAIC,MAAK,GAAI,IAAIC,eAAJ,CAAoBN,GAApB,CAAb;;IAEA,IAAIK,MAAM,CAACE,GAAP,CAAW,OAAX,CAAJ,EAAwB;MACpB,KAAKX,KAAL,GAAaS,MAAM,CAACE,GAAP,CAAW,OAAX,CAAb;MAEA,KAAKC,aAAL;IACJ;EACH,CAtBU;;EAuBXC,OAAO,EAAE;IACLD,aAAa,GAAE;MACXlB,KAAI,CACCoB,IADL,CACU,0BADV,EACsC;QAAC,SAAS,KAAKd;MAAf,CADtC;IAEJ;;EAJK;AAvBE,CAAf","names":["axios","RemediosBox","name","components","data","remedios","query","mounted","document","title","uri","window","location","search","substring","params","URLSearchParams","get","performSearch","methods","post"],"sourceRoot":"","sources":["/Users/franciscosantander/Desktop/Cesfam/cesfam_vue/src/views/Search.vue"],"sourcesContent":["<template>\n    <div class=\"page-search\">\n        <div class=\"columns is-multiline\">\n            <div class=\"column is-12\">\n                <h1 class=\"title\">Busqueda</h1>\n\n                <h2 class=\"is-size-5 has-text-gray\">Resultados para: \"{{ query }}\"</h2>\n            </div>\n