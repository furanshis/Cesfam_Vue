{"ast":null,"code":"import axios from 'axios';\nimport CarritoItem from '@/components/CarritoItem.vue';\nexport default {\n  name: 'Carrito',\n  components: {\n    CarritoItem\n  },\n\n  data() {\n    return {\n      carrito: {\n        items: []\n      }\n    };\n  },\n\n  mounted() {\n    this.carrito = this.$store.state.carrito;\n  },\n\n  computed: {\n    cartTotalLength() {\n      return this.carrito.items.reduce((acc, curVal) => {\n        ///la funcion reduce() de js nos permite hacer un loop en todos los items del carrito, para asi poder incrementar el valor\n        return acc += curVal.quantity;\n      }, 0);\n    },\n\n    cartTotalPrice() {\n      return this.carrito.items.reduce((acc, curVal) => {\n        ///la funcion reduce() de js nos permite hacer un loop en todos los items del carrito, para asi poder incrementar el valor o el largo del carrito\n        return acc += curVal.remedio.precioRemedio * curVal.quantity;\n      }, 0);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0CA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,WAAP,MAAwB,8BAAxB;AACA,eAAe;EACXC,IAAI,EAAE,SADK;EAEXC,UAAU,EAAE;IACRF;EADQ,CAFD;;EAKXG,IAAI,GAAG;IACH,OAAM;MACFC,OAAO,EAAE;QACLC,KAAK,EAAE;MADF;IADP,CAAN;EAKH,CAXU;;EAYXC,OAAO,GAAE;IACL,KAAKF,OAAL,GAAe,KAAKG,MAAL,CAAYC,KAAZ,CAAkBJ,OAAjC;EACH,CAdU;;EAeXK,QAAQ,EAAC;IACLC,eAAe,GAAE;MACb,OAAO,KAAKN,OAAL,CAAaC,KAAb,CAAmBM,MAAnB,CAA0B,CAACC,GAAD,EAAMC,MAAN,KAAiB;QAAE;QAChD,OAAOD,GAAE,IAAKC,MAAM,CAACC,QAArB;MACH,CAFM,EAEJ,CAFI,CAAP;IAGH,CALI;;IAMLC,cAAc,GAAE;MACZ,OAAO,KAAKX,OAAL,CAAaC,KAAb,CAAmBM,MAAnB,CAA0B,CAACC,GAAD,EAAMC,MAAN,KAAiB;QAAE;QAChD,OAAOD,GAAE,IAAKC,MAAM,CAACG,OAAP,CAAeC,aAAf,GAA+BJ,MAAM,CAACC,QAApD;MACH,CAFM,EAEJ,CAFI,CAAP;IAGH;;EAVI;AAfE,CAAf","names":["axios","CarritoItem","name","components","data","carrito","items","mounted","$store","state","computed","cartTotalLength","reduce","acc","curVal","quantity","cartTotalPrice","remedio","precioRemedio"],"sourceRoot":"","sources":["/Users/franciscosantander/Desktop/Cesfam/cesfam_vue/src/views/Carrito.vue"],"sourcesContent":["<template>\n    <div class=\"page-cart\">\n        <div class=\"column is-12\">\n            <h1 class=\"title\">Carrito</h1>\n        </div>\n\n        <div class=\"column is-12 box\">\n            <table class=\"table is-fullwidth\" v-if=\"cartTotalLength\">\n                <thead>\n                    <tr>\n                        <th>Remedio</th>\n                        <th>Precio</th>\n                        <th>Cantidad</th>\n                        <th>Total</th>\n                        <th></th>\n                    </tr>\n                </thead>\n\n                <tbody>\n                    <CarritoItem\n                        v-for=\"item in carrito.items\"\n                        v-bind:key=\"item.remedio.id\"\n                        v-bind:initialItem=\"item\"/>\n                </tbody>\n            </table>\n\n            <p v-else>todav√≠a no haz agregado nada al carrito....</p>\n        </div>\n        <div class=\"column\">\n            <h2 class=\"subtitle\">Summary</h2>\n\n            <strong>${{ cartTotalPrice }}</strong>, {{ cartTotalLength }} remedios\n\n            <hr>\n\n            <router-link to=\"/carrito/pago\" class=\"button is-dark\">Proceder al Pago</router-link>\n        </div>\n        \n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport CarritoItem from '@/components/CarritoItem.vue'\nexport default {\n    name: 'Carrito',\n    components: {\n        CarritoItem\n    },\n    data() {\n        return{\n            carrito: {\n                items: []\n            }\n        }\n    },\n    mounted(){\n        this.carrito = this.$store.state.carrito\n    },\n    computed:{\n        cartTotalLength(){\n            return this.carrito.items.reduce((acc, curVal) => { ///la funcion reduce() de js nos permite hacer un loop en todos los items del carrito, para asi poder incrementar el valor\n                return acc += curVal.quantity\n            }, 0)\n        },\n        cartTotalPrice(){\n            return this.carrito.items.reduce((acc, curVal) => { ///la funcion reduce() de js nos permite hacer un loop en todos los items del carrito, para asi poder incrementar el valor o el largo del carrito\n                return acc += curVal.remedio.precioRemedio * curVal.quantity\n            }, 0)\n        },\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}