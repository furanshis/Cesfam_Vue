{"ast":null,"code":"import axios from 'axios';\nimport { toast } from 'bulma-toast';\nexport default {\n  name: \"Detalle_remedio\",\n\n  data() {\n    return {\n      remedio: {},\n      quantity: 1\n    };\n  },\n\n  mounted() {\n    this.getRemedio();\n  },\n\n  methods: {\n    getRemedio() {\n      const categoria_slug = this.$route.params.categoria_slug;\n      const remedio_slug = this.$route.params.remedio_slug;\n      axios.get(`/api/v1/remedios/${categoria_slug}/${remedio_slug}`).then(response => {\n        this.remedio = response.data;\n        document.title = this.remedio.nombreRemedio + ' | Cesfam';\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n\n    addToCart() {\n      if (isNaN(this.quantity) || this.quantity < 1) {\n        this.quantity = 1;\n      }\n\n      const item = {\n        remedio: this.remedio,\n        quantity: this.quantity\n      };\n      this.$store.commit('addToCart', item);\n      toast({\n        message: 'El producto fue añadido al carrito',\n        type: 'is-success',\n        dismissible: true,\n        pauseOnHover: true,\n        duration: 2000,\n        position: 'bottom-right'\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmCA,OAAOA,KAAP,MAAkB,OAAlB;AACA,SAASC,KAAT,QAAsB,aAAtB;AACA,eAAe;EACXC,IAAI,EAAE,iBADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACHC,OAAO,EAAE,EADN;MAEHC,QAAQ,EAAE;IAFP,CAAP;EAIH,CAPU;;EAQXC,OAAO,GAAG;IACN,KAAKC,UAAL;EACH,CAVU;;EAWXC,OAAO,EAAE;IACLD,UAAU,GAAE;MACR,MAAME,cAAa,GAAI,KAAKC,MAAL,CAAYC,MAAZ,CAAmBF,cAA1C;MACA,MAAMG,YAAW,GAAI,KAAKF,MAAL,CAAYC,MAAZ,CAAmBC,YAAxC;MAEAZ,KAAI,CACCa,GADL,CACU,oBAAmBJ,cAAe,IAAGG,YAAa,EAD5D,EAEKE,IAFL,CAEUC,QAAO,IAAI;QACb,KAAKX,OAAL,GAAeW,QAAQ,CAACZ,IAAxB;QAEAa,QAAQ,CAACC,KAAT,GAAiB,KAAKb,OAAL,CAAac,aAAb,GAA6B,WAA9C;MACH,CANL,EAOKC,KAPL,CAOWC,KAAI,IAAI;QACXC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MACH,CATL;IAUH,CAfI;;IAgBLG,SAAS,GAAE;MACP,IAAIC,KAAK,CAAC,KAAKnB,QAAN,CAAL,IAAwB,KAAKA,QAAL,GAAgB,CAA5C,EAA8C;QAC1C,KAAKA,QAAL,GAAgB,CAAhB;MACJ;;MAEA,MAAMoB,IAAG,GAAI;QACTrB,OAAO,EAAE,KAAKA,OADL;QAETC,QAAQ,EAAE,KAAKA;MAFN,CAAb;MAKA,KAAKqB,MAAL,CAAYC,MAAZ,CAAmB,WAAnB,EAAgCF,IAAhC;MAEAxB,KAAK,CAAC;QACF2B,OAAO,EAAE,oCADP;QAEFC,IAAI,EAAE,YAFJ;QAGFC,WAAW,EAAE,IAHX;QAIFC,YAAY,EAAE,IAJZ;QAKFC,QAAQ,EAAE,IALR;QAMFC,QAAQ,EAAE;MANR,CAAD,CAAL;IAQJ;;EApCK;AAXE,CAAf","names":["axios","toast","name","data","remedio","quantity","mounted","getRemedio","methods","categoria_slug","$route","params","remedio_slug","get","then","response","document","title","nombreRemedio","catch","error","console","log","addToCart","isNaN","item","$store","commit","message","type","dismissible","pauseOnHover","duration","position"],"sourceRoot":"","sources":["/Users/franciscosantander/Desktop/Cesfam/cesfam_vue/src/views/Detalle_remedio.vue"],"sourcesContent":["<template>\n    <div class=\"detalle_remedio\">\n        <div class=\"columns is-multiline\">\n            <div class=\"column is-9\">\n                <figure class=\"image mb-6\">\n                    <img v-bind:src=\"remedio.get_image\">\n                </figure>\n\n                <h1 class=\"title\">Descripción</h1>\n                <p>{{ remedio.descripcionRemedio }}</p>\n            </div>\n            <div class=\"column is-3\">\n                <h1 class=\"title\">Información</h1>\n                <h2 class=\"subtitle\">{{ remedio.nombreRemedio }}</h2>\n                <p><strong>Stock: </strong>{{ remedio.stockRemedio }}</p>\n                <p><strong>Precio: </strong>$ {{remedio.precioRemedio }}</p>\n                <p><strong>Cantidad: </strong>{{ remedio.cantidadRemedio }}</p>\n                \n                <div class=\"field has-addons mt-6\"> <!-- el has-addons indica que el botón y el input van a estar juntos el uno del otro -->\n                    <div class=\"control\">\n                        <input type=\"number\" class=\"input\" min=\"1\" v-model=\"quantity\">\n                    </div>\n\n                    <div class=\"add-to-cart\"><button class=\"btn btn-primary\" data-disable-with=\"Agregando ...\"\n                                id=\"add-to-cart-button\" @click=\"addToCart\">Añadir al carro<span><img\n                                        src=\"https://d1tjllbjmslitt.cloudfront.net/assets/shopping_cart_icon_white-e9d947ffee21ece11fe275b55f3ebc35cd7979255ec6d4f4bdda66721efa350e.svg\"\n                                        alt=\"Shopping cart icon white\" width=\"15\" height=\"15\"></span></button>\n                        </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport axios from 'axios'\nimport { toast } from 'bulma-toast'\nexport default {\n    name: \"Detalle_remedio\",\n    data() {\n        return {\n            remedio: {},\n            quantity: 1\n        }\n    },\n    mounted() {\n        this.getRemedio()\n    },\n    methods: {\n        getRemedio(){\n            const categoria_slug = this.$route.params.categoria_slug\n            const remedio_slug = this.$route.params.remedio_slug\n\n            axios\n                .get(`/api/v1/remedios/${categoria_slug}/${remedio_slug}`)\n                .then(response =>{\n                    this.remedio = response.data\n\n                    document.title = this.remedio.nombreRemedio + ' | Cesfam'\n                })\n                .catch(error =>{\n                    console.log(error)\n                })\n        },\n        addToCart(){\n            if (isNaN(this.quantity) || this.quantity < 1){\n                this.quantity = 1\n            }\n\n            const item = {\n                remedio: this.remedio,\n                quantity: this.quantity,\n            }\n\n            this.$store.commit('addToCart', item)\n\n            toast({\n                message: 'El producto fue añadido al carrito',\n                type: 'is-success',\n                dismissible: true,\n                pauseOnHover: true,\n                duration: 2000,\n                position: 'bottom-right',\n            })\n        }\n    },\n}\n</script>"]},"metadata":{},"sourceType":"module"}